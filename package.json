		"gulp": "^5.0.0",
		"gulp-babel": "^8.0.0",
		"gulp-concat": "^2.6.1",
		"gulp-csso": "^4.0.1",
		"gulp-esbuild": "^0.14.0",
		"gulp-file": "^0.4.0",
		"gulp-sass": "^5.1.0",
		"laravel-echo": "^1.16.1",
		"laravel-vite-plugin": "^2.0.0",
		"lodash": "^4.17.21",
		"postcss": "^8.4.45",
		"pusher-js": "^8.4.0-rc2",
		"react-native": "^0.73.0",
		"sass": "^1.62.0",
		"tailwindcss": "^3.4.15",
		"tailwindcss-motion": "^1.1.1",
		"vite": "^7.1.3",
		"vite-plugin-mkcert": "^1.17.8"
	},
	"dependencies": {
