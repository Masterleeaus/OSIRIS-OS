{
	"private": true,
	"scripts": {
		"lint": "eslint ./resources/views/page",
		"dev": "vite",
		"build": "vite build",
		"clean": "vite clean",
		"watch": "gulp watch",
		"test": "jest",
		"test:watch": "jest --watch",
		"dev:ios": "cross-env PLATFORM=ios vite build --mode development",
		"build:ios": "cross-env PLATFORM=ios vite build --mode production",
		"dev:macos": "cross-env PLATFORM=macos electron dev",
		"build:macos": "cross-env PLATFORM=macos electron build",
		"dev:windows": "cross-env PLATFORM=windows electron dev",
		"build:windows": "cross-env PLATFORM=windows electron build",
		"dev:linux": "cross-env PLATFORM=linux vite build --mode development",
		"build:linux": "cross-env PLATFORM=linux vite build --mode production",
		"dev:web": "vite",
		"build:web": "vite build",
		"dev:winuwp": "cross-env PLATFORM=winuwp vite build --mode development",
		"build:winuwp": "cross-env PLATFORM=winuwp vite build --mode production",
		"dev:aurora": "cross-env PLATFORM=aurora vite build --mode development",
		"build:aurora": "cross-env PLATFORM=aurora vite build --mode production",
		"dev:android": "cross-env PLATFORM=android react-native start",
		"build:android": "cross-env PLATFORM=android react-native build",
		"dev:tizen": "cross-env PLATFORM=tizen tizen run -p",
		"build:tizen": "cross-env PLATFORM=tizen tizen package -t wgt",
		"dev:harmonyos": "cross-env PLATFORM=harmonyos hvigorw dev",
		"build:harmonyos": "cross-env PLATFORM=harmonyos hvigorw assembleHap",
		"dev:flutter": "cross-env PLATFORM=flutter flutter run",
		"build:flutter": "cross-env PLATFORM=flutter flutter build apk",
		"dev:tvos": "cross-env PLATFORM=tvos cap run tvos",
		"build:tvos": "cross-env PLATFORM=tvos cap build tvos",
		"deploy:github": "gh-pages -d dist",
		"deploy:gitlab": "echo 'Deploy to GitLab Pages'",
		"dev:vr-ar": "cross-env PLATFORM=vr-ar webpack serve --mode development",
		"build:vr-ar": "cross-env PLATFORM=vr-ar webpack build --mode production",
		"dev:iot": "cross-env PLATFORM=iot nodemon server/iot-gateway.js",
		"build:iot": "cross-env PLATFORM=iot pkg server/iot-gateway.js --out-path builds/iot",
		"dev:embedded": "cross-env PLATFORM=embedded arduino-cli compile",
		"build:embedded": "cross-env PLATFORM=embedded arduino-cli compile && arduino-cli upload",
		"dev:quantum": "cross-env PLATFORM=quantum jupyter lab",
		"build:quantum": "cross-env PLATFORM=quantum python setup.py build_ext --inplace",
		"setup:all-platforms": "npm run setup:desktop && npm run setup:mobile && npm run setup:vr-ar && npm run setup:iot && npm run setup:embedded",
		"setup:desktop": "npm run setup:macos && npm run setup:windows && npm run setup:linux",
		"setup:mobile": "npm run setup:ios && npm run setup:android && npm run setup:winuwp",
		"setup:macos": "electron-builder install-app-deps",
		"setup:windows": "electron-builder install-app-deps",
		"setup:linux": "sudo apt install -y build-essential",
		"setup:ios": "npm install @capacitor/core @capacitor/ios && npx cap add ios",
		"setup:android": "npm install @capacitor/core @capacitor/android && npx cap add android",
		"setup:winuwp": "npm install @capacitor/core @capacitor/windows && npx cap add windows",
		"setup:vr-ar": "npm install three @webxr/api aframe",
		"setup:iot": "npm install mqtt aws-iot-device-sdk azure-iot-device",
		"setup:embedded": "npm install johnny-five rpio pi-gpio",
		"setup:quantum": "pip install qiskit cirq amazon-braket-quantum",
		"quantum:shor": "python quantum/shor_algorithm.py",
		"quantum:grover": "python quantum/grover_search.py",
		"quantum:train": "python quantum/quantum_training.py",
		"metaverse:create": "node tools/create-metaverse.js",
		"metaverse:deploy": "node tools/deploy-metaverse.js",
		"blockchain:bridge": "node bridges/cross-chain/bridge.js",
		"blockchain:deploy": "npx hardhat run scripts/deploy.js",
		"ai:train": "python ai/federated_learning.py",
		"ai:marketplace": "node ai/marketplace/server.js",
		"edge:deploy": "ansible-playbook edge-deployment.yml",
		"edge:monitor": "node tools/edge-monitor.js",
		"security:audit": "npm run security:quantum && npm run security:blockchain",
		"security:quantum": "node tools/quantum-security-audit.js",
		"security:blockchain": "npx hardhat run scripts/security-audit.js",
		"compliance:check": "node tools/compliance-checker.js",
		"performance:test": "npm run test:quantum && npm run test:cross-chain && npm run test:ai",
		"test:quantum": "python -m pytest quantum/tests/",
		"test:cross-chain": "npx hardhat test bridges/tests/",
		"test:ai": "python -m pytest ai/tests/",
		"docs:generate": "node tools/generate-docs.js",
		"docs:serve": "npx docsify serve docs/",
		"deploy:production": "npm run build:web && npm run deploy:github",
		"deploy:staging": "npm run build:web && npm run deploy:gitlab",
		"deploy:edge": "npm run build:iot && npm run edge:deploy",
		"monitor:health": "node tools/health-monitor.js",
		"backup:create": "node tools/create-backup.js",
		"backup:restore": "node tools/restore-backup.js",
		"ci:build-all": "npm run build:web && npm run build:ios && npm run build:macos && npm run build:windows && npm run build:linux && npm run build:winuwp && npm run build:aurora && npm run build:android && npm run build:tizen && npm run build:harmonyos && npm run build:flutter && npm run build:tvos && npm run build:vr-ar && npm run build:iot && npm run build:embedded"
	},
	"devDependencies": {
		"@angular-eslint/template-parser": "^17.2.1",
